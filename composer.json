{
    "name": "forestadmin/agent-php",
    "description": "The official agent PHP for Forest",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0",
        "illuminate/collections": "^9.13",
        "spatie/enum": "^3.13",
        "slim/slim": "4.*",
        "slim/psr7": "^1.5",
        "php-di/slim-bridge": "^3.2",
        "symfony/cache": "^6.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Forestadmin\\AgentPHP\\": "src/"
        },
        "files": [
            "src/Agent/Utils/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Forestadmin\\AgentPHP\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
