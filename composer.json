{
    "name": "forestadmin/agent-php",
    "description": "The official agent PHP for Forest",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20"
    },
    "autoload": {
        "psr-4": {
            "Forestadmin\\AgentPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forestadmin\\AgentPHP\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
