{
    "name": "forestadmin/agent-php",
    "description": "The official agent PHP for Forest",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0",
        "firebase/php-jwt": "^6.2",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/collections": "^9.13",
        "league/oauth2-client": "^2.6",
        "spatie/enum": "^3.13",
        "symfony/cache": "^6.0",
        "symfony/http-foundation": "^6.0",
        "composer/composer": "^2.3",
        "league/oauth2-client": "^2.6",
        "firebase/php-jwt": "^6.2",
        "nesbot/carbon": "^2.58",
        "php-di/php-di": "^6.4",
        "league/fractal": "^0.20.1",
        "galbar/jsonpath": "^2.0",
        "illuminate/support": "^9.19"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.21",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "ForestAdmin\\AgentPHP\\": "src/"
        },
        "files": [
            "src/Agent/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Forestadmin\\AgentPHP\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
