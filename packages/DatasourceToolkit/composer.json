{
    "name": "forestadmin/php-datasource-toolkit",
    "description": "Subtree split of the PHP Datasource Toolkit component",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "version": "1.8.0",
    "require": {
      "php": "^8.0",
      "illuminate/collections": "^8.0|^9.0|^10.0",
      "illuminate/support": "^8.0|^9.0|^10.0",
      "nesbot/carbon": "^2.58",
      "forestadmin/php-agent-toolkit": "^1.0.0",
      "forestadmin/php-datasource-customizer": "^1.0.0"
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer": "^3.0",
      "pestphp/pest": "^1.21",
      "pestphp/pest-plugin-mock": "^1.0",
      "phpspec/prophecy-phpunit": "^2.0",
      "symfony/var-dumper": "^5.4|^6.0",
      "spatie/pest-plugin-test-time": "^1.1"
    },
    "autoload": {
        "psr-4": {
          "ForestAdmin\\AgentPHP\\DatasourceToolkit\\": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "ForestAdmin\\AgentPHP\\Tests\\": "tests"
      }
    },
    "config": {
      "allow-plugins": {
        "pestphp/pest-plugin": true
      }
    }
}
