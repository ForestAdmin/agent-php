{
    "name": "forestadmin/php-agent-toolkit",
    "description": "Subtree split of the PHP Agent Toolkit component",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "version": "1.6.1",
    "require": {
        "php": "^8.0",
        "firebase/php-jwt": "^6.2",
        "galbar/jsonpath": "^2.0",
        "guzzlehttp/guzzle": "^6.0|^7.4",
        "illuminate/collections": "^8.0|^9.0|^10.0",
        "illuminate/database": "^v8.83|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "league/csv": "^9.8",
        "league/fractal": "^0.20.1",
        "league/oauth2-client": "^2.6",
        "mlocati/ip-lib": "^1.18",
        "nesbot/carbon": "^2.58",
        "php-di/php-di": "^7.0",
        "ramsey/uuid": "^3.9.7|^4.3",
        "symfony/cache": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "forestadmin/php-datasource-customizer": "^1.0.0",
        "forestadmin/php-datasource-toolkit": "^1.0.0",
        "forestadmin/php-base-datasource": "^1.0.0",
        "monolog/monolog": "^2.5|3.0",
        "laravel/serializable-closure": "^1.3"
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer": "^3.0",
      "pestphp/pest": "^1.21",
      "pestphp/pest-plugin-mock": "^1.0",
      "phpspec/prophecy-phpunit": "^2.0",
      "symfony/var-dumper": "^5.4|^6.0",
      "spatie/pest-plugin-test-time": "^1.1"
    },
    "autoload": {
      "files": [
        "src/functions.php"
      ],
      "psr-4": {
          "ForestAdmin\\AgentPHP\\Agent\\": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "ForestAdmin\\AgentPHP\\Tests\\": "tests"
      }
    },
    "config": {
      "allow-plugins": {
        "pestphp/pest-plugin": true
      }
    }
}
